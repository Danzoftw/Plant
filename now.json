{
    "version": 2,
    "name": "Plantisserie",
    "asias": "Plantisserie",
    "builds": [
        {
            "src": "package.json",
            "use": "@now/next"
        }
    ],
    "routes":[
        {
            "src": "/product/(?<slug[^/]+)$",
            "dest": "/product?slug=$slug"
        }
        
    ]
}